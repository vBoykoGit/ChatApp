{
    "search.exclude": {
        "# See https://help.github.com/ignore-files/ for more about ignoring files.": true,
        "# dependencies": true,
        "/node_modules": true,
        "# testing": true,
        "/coverage": true,
        "# production": true,
        "/build": true,
        "# misc": true,
        ".DS_Store": true,
        ".env.local": true,
        ".env.development.local": true,
        ".env.test.local": true,
        ".env.production.local": true,
        "npm-debug.log*": true,
        "yarn-debug.log*": true,
        "yarn-error.log*": true,
        "# vscode": true,
        ".vscode/*": true,
        "!.vscode/settings.json": true,
        "!.vscode/tasks.json": true,
        "!.vscode/launch.json": true,
        "!.vscode/extensions.json": true,
        "# css": true,
        ".sass-cache/": true,
        "*.css.map": true,
        "*.sass.map": true,
        "*.scss.map": true,
        "src/css/*.css": true
    },
    "files.exclude": {
        "# See https://help.github.com/ignore-files/ for more about ignoring files.": true,
        "# dependencies": true,
        "/node_modules": true,
        "# testing": true,
        "/coverage": true,
        "# production": true,
        "/build": true,
        "# misc": true,
        ".DS_Store": true,
        ".env.local": true,
        ".env.development.local": true,
        ".env.test.local": true,
        ".env.production.local": true,
        "npm-debug.log*": true,
        "yarn-debug.log*": true,
        "yarn-error.log*": true,
        "# vscode": true,
        ".vscode/*": true,
        "!.vscode/settings.json": true,
        "!.vscode/tasks.json": true,
        "!.vscode/launch.json": true,
        "!.vscode/extensions.json": true,
        "# css": true,
        ".sass-cache/": true,
        "*.css.map": true,
        "*.sass.map": true,
        "*.scss.map": true,
        "src/css/*.css": true,
        "node_modules": true,
        "coverage": true,
        "build": true,
        "**/.DS_Store": true,
        "**/.env.local": true,
        "**/.env.development.local": true,
        "**/.env.test.local": true,
        "**/.env.production.local": true,
        "**/npm-debug.log*": true,
        "**/yarn-debug.log*": true,
        "**/yarn-error.log*": true,
        ".vscode/settings.json": false,
        ".vscode/tasks.json": false,
        ".vscode/launch.json": false,
        ".vscode/extensions.json": false,
        "**/.sass-cache/": true,
        "**/*.css.map": true,
        "**/*.sass.map": true,
        "**/*.scss.map": true,
        "**/*.css": true
    }
}